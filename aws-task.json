{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "MyBucket": {
            "Type": "AWS::S3::Bucket",
            "Properties": {}
      },
        "MyVPC": {
            "Type": "AWS::EC2::VPC",
            "Properties": {
                "CidrBlock": "10.0.0.0/16"
            },
      },
      "Subnet1": {
        "Type": "AWS::EC2::Subnet",
        "Properties": {
           "VpcId": { "Ref": "MyVPC" },
           "CidrBlock": "10.0.0.0/20"
        }
      },
      "Subnet2": {
        "Type": "AWS::EC2::Subnet",
        "Properties": {
           "VpcId": { "Ref": "MyVPC" },
           "CidrBlock": "10.0.16.0/20"
        }
      },
      "Subnet3": {
        "Type": "AWS::EC2::Subnet",
        "Properties": {
          "VpcId": { "Ref": "MyVPC" },
          "CidrBlock": "10.0.32.0/20"
        }
      },
      "RouteTable": {
        "Type": "AWS::EC2::RouteTable",
        "Properties": {
          "VpcId": { "Ref": "MyVPC" }
        }
      }
   }
}
